con=\"el-icon-plus\"\r\n        @click=\"handleCreate\"\r\n      >\r\n        {{ $t('common.add') }}\r\n      </el-button>\r\n      <el-button\r\n        v-waves\r\n        :loading=\"downloadLoading\"\r\n        class=\"filter-item\"\r\n        type=\"primary\"\r\n        icon=\"el-icon-download\"\r\n        @click=\"handleDownload\"\r\n      >\r\n        {{ $t('common.export') }}\r\n      </el-button>\r\n      <el-checkbox\r\n        v-model=\"showUnitAddr\"\r\n        class=\"filter-item\"\r\n        style=\"margin-left:15px;\"\r\n      >\r\n        {{ $t('customer.englishLevel') }}\r\n      </el-checkbox>\r\n      <el-checkbox\r\n        v-model=\"showDetailAddr\"\r\n        class=\"filter-item\"\r\n        style=\"margin-left:15px;\"\r\n      >\r\n        {{ $t('customer.computerLevel') }}\r\n      </el-checkbox>\r\n    </div>\r\n    <el-table\r\n      v-loading=\"listLoading\"\r\n      :data=\"list\"\r\n      border\r\n      fit\r\n      highlight-current-row\r\n      style=\"width: 100%;\"\r\n    >\r\n      <el-table-column :label=\"$t('customer.schoolName')\">\r\n        <template slot-scope=\"{row}\">\r\n          <span>{{ row.schoolName }}</span>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column :label=\"$t('customer.majorName')\">\r\n        <template slot-scope=\"{row}\">\r\n          <span>{{ row.majorName }}</span>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column :label=\"$t('customer.schoolAddres')\">\r\n        <template slot-scope=\"{row}\">\r\n          <span>{{ row.schoolAddres}}</span>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column\r\n        :label=\"$t('customer.englishLevel')\"\r\n        v-if=\"showDetailAddr\"\r\n      >\r\n        <template slot-scope=\"{row}\">\r\n          <span>{{ row.englishLevel }}</span>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column\r\n        :label=\"$t('customer.computerLevel')\"\r\n        v-if=\"showUnitAddr\"\r\n      >\r\n        <template slot-scope=\"{row}\">\r\n          <span>{{ row.computerLevel }}</span>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column\r\n        :label=\"$t('customer.entranceTime')\"\r\n        min-width=\"100px\"\r\n      >\r\n        <template slot-scope=\"{row}\">\r\n          <span>{{ row.entranceTime | parseTime('{y}-{m}-{d} {h}:{i}:{s}') }}</span>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column\r\n        :label=\"$t('customer.gradTime')\"\r\n        min-width=\"100px\"\r\n      >\r\n        <template slot-scope=\"{row}\">\r\n          <span>{{ row.gradTime | parseTime('{y}-{m}-{d} {h}:{i}:{s}') }}</span>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column\r\n        :label=\"$t('common.operate')\"\r\n        min-width=\"100px\"\r\n        align=\"center\"\r\n        class-name=\"small-padding fixed-width\"\r\n      >\r\n        <template slot-scope=\"{row,$index}\">\r\n          <el-button\r\n            type=\"primary\"\r\n            size=\"mini\"\r\n            style=\"margin-right: 5px\"\r\n            @click=\"handleUpdate(row)\"\r\n          >\r\n            {{ $t('common.edit') }}\r\n          </el-button>\r\n          <el-popover\r\n            :ref=\"'popover-' + row.id\"\r\n            placement=\"top\"\r\n            width=\"160\"\r\n            :title=\"$t('common.confirm-delete')\"\r\n            trigger=\"click\"\r\n          >\r\n            <div style=\"text-align: right; margin: 0\">\r\n              <el-button\r\n                size=\"mini\"\r\n                @click=\"$refs[`popover-` + row.id].doClose()\"\r\n              >{{ $t('common.cancel') }}</el-button>\r\n              <el-button\r\n                type=\"primary\"\r\n                size=\"mini\"\r\n                @click=\"$refs[`popover-` + row.id].doClose();handleDelete(row,$index)\"\r\n              >{{ $t('common.confirm') }}</el-button>\r\n            </div>\r\n            <el-button\r\n              slot=\"reference\"\r\n              type=\"danger\"\r\n              size=\"mini\"\r\n            >\r\n              {{ $t('common.delete') }}\r\n            </el-button>\r\n          </el-popover>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n    <el-dialog\r\n      :title=\"textMap[dialogStatus]\"\r\n      :visible.sync=\"dialogFormVisible\"\r\n    >\r\n      <el-form\r\n        ref=\"dataForm\"\r\n        :rules=\"rules\"\r\n        label-position=\"right\"\r\n        label-width=\"100px\"\r\n        style=\"width: 100%\"\r\n      >\r\n        <el-form-item :label=\"$t('customer.schoolName')\">\r\n          <el-input\r\n            v-model=\"temp.schoolName\"\r\n            :placeholder=\"$t('common.enter')\"\r\n            minlength=\"3\"\r\n            maxlength=\"64\"\r\n            clearable\r\n          />\r\n        </el-form-item>\r\n        <el-form-item :label=\"$t('customer.majorName')\">\r\n          <el-input\r\n            v-model=\"temp.majorName\"\r\n            :placeholder=\"$t('common.enter')\"\r\n            minlength=\"3\"\r\n            maxlength=\"64\"\r\n            clearable\r\n          />\r\n        </el-form-item>\r\n        <el-form-item :label=\"$t('customer.schoolAddres')\">\r\n          <el-input\r\n            v-model=\"temp.schoolAddres\"\r\n            :placeholder=\"$t('common.enter')\"\r\n            minlength=\"3\"\r\n            maxlength=\"64\"\r\n            clearable\r\n          />\r\n        </el-form-item>\r\n        <el-form-item :label=\"$t('customer.englishLevel')\">\r\n          <el-input\r\n            v-model=\"temp.englishLevel\"\r\n            :placeholder=\"$t('common.enter')\"\r\n            minlength=\"3\"\r\n            maxlength=\"64\"\r\n            clearable\r\n          />\r\n        </el-form-item>\r\n        <el-form-item :label=\"$t('customer.computerLevel')\">\r\n          <el-input\r\n            v-model=\"temp.computerLevel\"\r\n            :placeholder=\"$t('common.enter')\"\r\n            minlength=\"3\"\r\n            maxlength=\"64\"\r\n            clearable\r\n          />\r\n        </el-form-item>\r\n        <el-form-item :label=\"$t('customer.entranceTime')\">\r\n          <el-date-picker\r\n            v-model=\"temp.entranceTime\"\r\n            type=\"datetime\"\r\n            :placeholder=\"$t('common.enter')\"\r\n            style=\"width: 100%\"\r\n          />\r\n        </el-form-item>\r\n        <el-form-item :label=\"$t('customer.gradTime')\">\r\n          <el-date-picker\r\n            v-model=\"temp.gradTime\"\r\n            type=\"datetime\"\r\n            :placeholder=\"$t('common.enter')\"\r\n            style=\"width: 100%\"\r\n          />\r\n        </el-form-item>\r\n      </el-form>\r\n      <div\r\n        slot=\"footer\"\r\n        class=\"dialog-footer\"\r\n      >\r\n        <el-button @click=\"dialogFormVisible = false\">\r\n          {{ $t('common.cancel') }}\r\n        </el-button>\r\n        <el-button\r\n          type=\"primary\"\r\n          @click=\"dialogStatus==='create'?createData():updateData()\"\r\n        >\r\n          {{ $t('common.confirm') }}\r\n        </el-button>\r\n      </div>\r\n    </el-dialog>\r\n  </div>\r\n\r\n</template>\r\n\r\n<script>\r\nimport waves from '@/directive/waves' // waves directive\r\n// eslint-disable-next-line no-unused-vars\r\nimport { parseTime } from '@/utils'\r\n// eslint-disable-next-line no-unused-vars\r\nimport { getEducation, upEducation, chaneEducation, delEducation } from '@/api/getUser-info/education'\r\n\r\nexport default {\r\n\r\n  name: 'Work',\r\n  directives: { waves },\r\n  data () {\r\n    return {\r\n      list: [],\r\n      temp: {\r\n        id: undefined\r\n      },\r\n      dialogFormVisible: false,\r\n      dialogStatus: '',\r\n      textMap: {\r\n        update: this.$t('common.edit'),\r\n        create: this.$t('common.add')\r\n      },\r\n      rules: {\r\n\r\n      },\r\n      listLoading: false,\r\n      downloadLoading: false,\r\n      showUnitAddr: false,\r\n      showUnitContact: false,\r\n      showDetailAddr: false,\r\n      showUnitPhone: false\r\n    }\r\n  },\r\n\r\n  computed: {\r\n    NatureOptions () {\r\n      return [{\r\n        value: 'zhinan',\r\n        label: '指南'\r\n      }, {\r\n        value: 'jinan',\r\n        label: '济南'\r\n      }\r\n      ]\r\n    }\r\n  },\r\n  methods: {\r\n    resetTemp () {\r\n      this.temp = {\r\n        id: undefined\r\n      }\r\n    },\r\n    handleCreate () {\r\n      this.resetTemp()\r\n      this.dialogStatus = 'create'\r\n      this.dialogFormVisible = true\r\n      this.$nextTick(() => {\r\n        this.$refs['dataForm'].clearValidate()\r\n      })\r\n    },\r\n    // 新增教育经历\r\n    createData () {\r\n      // 转换为时间戳\r\n      if (this.temp.entranceTime !== undefined) {\r\n        this.temp['entranceTime'] = Date.parse(this.temp.entranceTime)\r\n      }\r\n      if (this.temp.gradTime !== undefined) {\r\n        this.temp['gradTime'] = Date.parse(this.temp.gradTime)\r\n      }\r\n      this.temp['consumerId'] = '22332110'\r\n      upEducation(this.temp).then(data => {\r\n        this.getUserId()\r\n        this.dialogFormVisible = false\r\n      }).catch(err => {\r\n        console.log(err)\r\n      })\r\n    },\r\n    // 删除教育经历\r\n    handleDelete (row) {\r\n      delEducation({\r\n        educationId: row.id\r\n      }).then(data => {\r\n        console.log(data)\r\n        this.getUserId()\r\n      }).catch(err => {\r\n        console.log(err)\r\n      })\r\n    },\r\n    // 弹窗\r\n    handleUpdate (row) {\r\n      this.temp = Object.assign({}, row) // copy obj\r\n      this.dialogStatus = 'update'\r\n      this.dialogFormVisible = true\r\n      this.$nextTick(() => {\r\n        this.$refs['dataForm'].clearValidate()\r\n      })\r\n    },\r\n\r\n    // 修改教育经历\r\n    updateData () {\r\n      // 判断时间格式\r\n      console.log(this.temp)\r\n      if (typeof this.temp.entranceTime !== 'number') {\r\n        if (this.temp.entranceTime === null) {\r\n          this.temp['entranceTime'] = null\r\n        } else {\r\n          this.temp['entranceTime'] = Date.parse(this.temp.entranceTime)\r\n        }\r\n      }\r\n      if (typeof this.temp.gradTime !== 'number') {\r\n        // console.log(this.temp.endTime === null)\r\n        if (this.temp.gradTime === null) {\r\n          this.temp['gradTime'] = null\r\n        } else {\r\n          this.temp['gradTime'] = Date.parse(this.temp.gradTime)\r\n        }\r\n      }\r\n      delete this.temp['updateTime']\r\n      delete this.temp['isCurrent']\r\n      delete this.temp['createTime']\r\n      chaneEducation(\r\n        this.temp\r\n      ).then(data => {\r\n        console.log(data)\r\n        this.getUserId()\r\n        this.dialogFormVisible = false\r\n      }).catch(err => {\r\n        console.log(err)\r\n      })\r\n    },\r\n    // 获取教育经历\r\n    getUserId () {\r\n      getEducation({\r\n        consumerId: this.$route.params.id\r\n      }).then(data => {\r\n        console.log(data)\r\n        this.list = data.data\r\n      }).catch(err => {\r\n        console.log(err)\r\n      })\r\n    }\r\n  },\r\n  mounted () {\r\n    this.getUserId()\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang='less' scoped>\r\n</style>\r\n"],"sourceRoot":"src/views/gather-info/UserDetails/views"}]}