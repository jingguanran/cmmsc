QAAA,OAAA,CAAA,GAAA,CAAA,IAAA;;AACA,QAAA,MAAA,CAAA,SAAA;AACA,OALA,EAKA,KALA,CAKA,UAAA,GAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA,OAPA;AAQA,KAzCA;AA0CA;AACA,IAAA,YA3CA,wBA2CA,GA3CA,EA2CA;AAAA;;AACA,WAAA,IAAA,GAAA,MAAA,CAAA,MAAA,CAAA,EAAA,EAAA,GAAA,CAAA,CADA,CACA;;AACA,WAAA,YAAA,GAAA,QAAA;AACA,WAAA,iBAAA,GAAA,IAAA;AACA,WAAA,SAAA,CAAA,YAAA;AACA,QAAA,MAAA,CAAA,KAAA,CAAA,UAAA,EAAA,aAAA;AACA,OAFA;AAGA,KAlDA;AAmDA;AACA,IAAA,UApDA,wBAoDA;AAAA;;AACA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,KAAA,IAAA;AACA,aAAA,KAAA,IAAA,CAAA,YAAA,CAAA;AACA,aAAA,KAAA,IAAA,CAAA,WAAA,CAAA;AACA,aAAA,KAAA,IAAA,CAAA,YAAA,CAAA;AACA,MAAA,aAAA,CACA,KAAA,IADA,CAAA,CAEA,IAFA,CAEA,UAAA,IAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,IAAA;;AACA,QAAA,MAAA,CAAA,SAAA;;AACA,QAAA,MAAA,CAAA,iBAAA,GAAA,KAAA;AACA,OANA,EAMA,KANA,CAMA,UAAA,GAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA,OARA;AASA,KAnEA;AAoEA;AACA,IAAA,SArEA,uBAqEA;AAAA;;AACA,MAAA,WAAA,CAAA;AACA,QAAA,UAAA,EAAA,KAAA,MAAA,CAAA,MAAA,CAAA;AADA,OAAA,CAAA,CAEA,IAFA,CAEA,UAAA,IAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,IAAA;AACA,QAAA,MAAA,CAAA,IAAA,GAAA,IAAA,CAAA,IAAA;AACA,OALA,EAKA,KALA,CAKA,UAAA,GAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA,OAPA;AAQA;AA9EA,GAtCA;AAsHA,EAAA,OAtHA,qBAsHA;AACA,SAAA,SAAA;AACA;AAxHA,CAAA","sourcesContent":["<template>\r\n  <div class=\"app-container\">\r\n    <div class=\"filter-container\">\r\n      <el-button\r\n        class=\"filter-item\"\r\n        style=\"margin-left: 10px;\"\r\n        type=\"primary\"\r\n        icon=\"el-icon-plus\"\r\n        @click=\"handleCreate\"\r\n      >\r\n        {{ $t('common.add') }}\r\n      </el-button>\r\n      <el-button\r\n        v-waves\r\n        :loading=\"downloadLoading\"\r\n        class=\"filter-item\"\r\n        type=\"primary\"\r\n        icon=\"el-icon-download\"\r\n        @click=\"handleDownload\"\r\n      >\r\n        {{ $t('common.export') }}\r\n      </el-button>\r\n    </div>\r\n    <el-table\r\n      v-loading=\"listLoading\"\r\n      :data=\"list\"\r\n      border\r\n      fit\r\n      highlight-current-row\r\n      style=\"width: 100%;\"\r\n    >\r\n      <el-table-column :label=\"$t('customer.childrenName')\">\r\n        <template slot-scope=\"{row}\">\r\n          <span>{{ row.childrenName }}</span>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column :label=\"$t('customer.childrenAge')\">\r\n        <template slot-scope=\"{row}\">\r\n          <span>{{ row.childrenAge }}</span>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column :label=\"$t('customer.childrenGender')\">\r\n        <template slot-scope=\"{row}\">\r\n          <span>{{ row.childrenGender}}</span>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column :label=\"$t('customer.childrenSchool')\">\r\n        <template slot-scope=\"{row}\">\r\n          <span>{{ row.childrenSchool }}</span>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column :label=\"$t('customer.childrenGrade')\">\r\n        <template slot-scope=\"{row}\">\r\n          <span>{{ row.childrenGrade }}</span>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column\r\n        :label=\"$t('common.operate')\"\r\n        min-width=\"100px\"\r\n        align=\"center\"\r\n        class-name=\"small-padding fixed-width\"\r\n      >\r\n        <template slot-scope=\"{row,$index}\">\r\n          <el-button\r\n            type=\"primary\"\r\n            size=\"mini\"\r\n            style=\"margin-right: 5px\"\r\n            @click=\"handleUpdate(row)\"\r\n          >\r\n            {{ $t('common.edit') }}\r\n          </el-button>\r\n          <el-popover\r\n            :ref=\"'popover-' + row.id\"\r\n            placement=\"top\"\r\n            width=\"160\"\r\n            :title=\"$t('common.confirm-delete')\"\r\n            trigger=\"click\"\r\n          >\r\n            <div style=\"text-align: right; margin: 0\">\r\n              <el-button\r\n                size=\"mini\"\r\n                @click=\"$refs[`popover-` + row.id].doClose()\"\r\n              >{{ $t('common.cancel') }}</el-button>\r\n              <el-button\r\n                type=\"primary\"\r\n                size=\"mini\"\r\n                @click=\"$refs[`popover-` + row.id].doClose();handleDelete(row,$index)\"\r\n              >{{ $t('common.confirm') }}</el-button>\r\n            </div>\r\n            <el-button\r\n              slot=\"reference\"\r\n              type=\"danger\"\r\n              size=\"mini\"\r\n            >\r\n              {{ $t('common.delete') }}\r\n            </el-button>\r\n          </el-popover>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n    <el-dialog\r\n      :title=\"textMap[dialogStatus]\"\r\n      :visible.sync=\"dialogFormVisible\"\r\n    >\r\n      <el-form\r\n        ref=\"dataForm\"\r\n        :rules=\"rules\"\r\n        label-position=\"right\"\r\n        label-width=\"100px\"\r\n        style=\"width: 100%\"\r\n      >\r\n        <el-form-item :label=\"$t('customer.childrenName')\">\r\n          <el-input\r\n            v-model=\"temp.childrenName\"\r\n            :placeholder=\"$t('common.enter')\"\r\n            minlength=\"3\"\r\n            maxlength=\"64\"\r\n            clearable\r\n          />\r\n        </el-form-item>\r\n        <el-form-item :label=\"$t('customer.childrenAge')\">\r\n          <el-input\r\n            v-model=\"temp.childrenAge\"\r\n            :placeholder=\"$t('common.enter')\"\r\n            minlength=\"3\"\r\n            maxlength=\"64\"\r\n            clearable\r\n          />\r\n        </el-form-item>\r\n        <el-form-item :label=\"$t('customer.childrenGender')\">\r\n          <el-select\r\n            v-model=\"temp.childrenGender\"\r\n            :placeholder=\"$t('common.choose')\"\r\n            clearable\r\n            class=\"filter-item\"\r\n            style=\"width: 100%\"\r\n          >\r\n            <el-option\r\n              v-for=\"item in NatureOptions\"\r\n              :key=\"item.value\"\r\n              :label=\"item.name\"\r\n              :value=\"item.value\"\r\n            />\r\n          </el-select>\r\n        </el-form-item>\r\n        <el-form-item :label=\"$t('customer.childrenSchool')\">\r\n          <el-input\r\n            v-model=\"temp.childrenSchool\"\r\n            :placeholder=\"$t('common.enter')\"\r\n            minlength=\"3\"\r\n            maxlength=\"64\"\r\n            clearable\r\n          />\r\n        </el-form-item>\r\n        <el-form-item :label=\"$t('customer.childrenGrade')\">\r\n          <el-input\r\n            v-model=\"temp.childrenGrade\"\r\n            :placeholder=\"$t('common.enter')\"\r\n            minlength=\"3\"\r\n            maxlength=\"64\"\r\n            clearable\r\n          />\r\n        </el-form-item>\r\n      </el-form>\r\n      <div\r\n        slot=\"footer\"\r\n        class=\"dialog-footer\"\r\n      >\r\n        <el-button @click=\"dialogFormVisible = false\">\r\n          {{ $t('common.cancel') }}\r\n        </el-button>\r\n        <el-button\r\n          type=\"primary\"\r\n          @click=\"dialogStatus==='create'?createData():updateData()\"\r\n        >\r\n          {{ $t('common.confirm') }}\r\n        </el-button>\r\n      </div>\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n<script>\r\nimport waves from '@/directive/waves' // waves directive\r\n// eslint-disable-next-line no-unused-vars\r\nimport { parseTime } from '@/utils'\r\n// eslint-disable-next-line no-unused-vars\r\nimport { getChildren, upChildren, chaneChildren, delChildren } from '@/api/getUser-info/children'\r\nexport default {\r\n  name: 'Work',\r\n  directives: { waves },\r\n  data () {\r\n    return {\r\n      list: [],\r\n      temp: {\r\n        id: undefined\r\n      },\r\n      dialogFormVisible: false,\r\n      dialogStatus: '',\r\n      textMap: {\r\n        update: this.$t('common.edit'),\r\n        create: this.$t('common.add')\r\n      },\r\n      rules: {\r\n\r\n      },\r\n      listLoading: false,\r\n      downloadLoading: false,\r\n      showUnitAddr: false,\r\n      showUnitContact: false,\r\n      showDetailAddr: false,\r\n      showUnitPhone: false\r\n    }\r\n  },\r\n  computed: {\r\n    NatureOptions () {\r\n      return [{\r\n        value: '男',\r\n        label: '0'\r\n      }, {\r\n        value: '女',\r\n        label: '1'\r\n      }\r\n      ]\r\n    }\r\n  },\r\n  methods: {\r\n    resetTemp () {\r\n      this.temp = {\r\n        id: undefined\r\n      }\r\n    },\r\n    handleCreate () {\r\n      this.resetTemp()\r\n      this.dialogStatus = 'create'\r\n      this.dialogFormVisible = true\r\n      this.$nextTick(() => {\r\n        this.$refs['dataForm'].clearValidate()\r\n      })\r\n    },\r\n    // 新增子女信息\r\n    createData () {\r\n      // 转换为时间戳 \r\n      if (this.temp.startTime !== undefined) {\r\n        this.temp['startTime'] = Date.parse(this.temp.startTime)\r\n      }\r\n      if (this.temp.endTime !== undefined) {\r\n        this.temp['endTime'] = Date.parse(this.temp.endTime)\r\n      }\r\n      this.temp['consumerId'] = '22332110'\r\n      upChildren(this.temp).then(data => {\r\n        this.getUserId()\r\n        this.dialogFormVisible = false\r\n      }).catch(err => {\r\n        console.log(err)\r\n      })\r\n    },\r\n    // 删除子女信息\r\n    handleDelete (row) {\r\n      delChildren({\r\n        childrenId: row.id\r\n      }).then(data => {\r\n        console.log(data)\r\n        this.getUserId()\r\n      }).catch(err => {\r\n        console.log(err)\r\n      })\r\n    },\r\n    // 弹窗\r\n    handleUpdate (row) {\r\n      this.temp = Object.assign({}, row) // copy obj\r\n      this.dialogStatus = 'update'\r\n      this.dialogFormVisible = true\r\n      this.$nextTick(() => {\r\n        this.$refs['dataForm'].clearValidate()\r\n      })\r\n    },\r\n    // 修改子女信息\r\n    updateData () {\r\n      // 判断时间格式\r\n      console.log(this.temp)\r\n      delete this.temp['updateTime']\r\n      delete this.temp['isCurrent']\r\n      delete this.temp['createTime']\r\n      chaneChildren(\r\n        this.temp\r\n      ).then(data => {\r\n        console.log(data)\r\n        this.getUserId()\r\n        this.dialogFormVisible = false\r\n      }).catch(err => {\r\n        console.log(err)\r\n      })\r\n    },\r\n    // 获取子女信息\r\n    getUserId () {\r\n      getChildren({\r\n        consumerId: this.$route.params.id\r\n      }).then(data => {\r\n        console.log(data)\r\n        this.list = data.data\r\n      }).catch(err => {\r\n        console.log(err)\r\n      })\r\n    },\r\n  },\r\n  mounted () {\r\n    this.getUserId()\r\n  }\r\n}\r\n\r\n</script>\r\n\r\n<style lang='less' scoped>\r\n</style>\r\n"],"sourceRoot":"src/views/gather-info/UserDetails/views"}]}