"\r\n        style=\"margin-left:15px;\"\r\n      >\r\n        {{ $t('customer.relationAddress') }}\r\n      </el-checkbox>\r\n    </div>\r\n    <el-table\r\n      v-loading=\"listLoading\"\r\n      :data=\"list\"\r\n      border\r\n      fit\r\n      highlight-current-row\r\n      style=\"width: 100%;\"\r\n    >\r\n      <el-table-column :label=\"$t('customer.relationName')\">\r\n        <template slot-scope=\"{row}\">\r\n          <span>{{ row.relationName }}</span>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column :label=\"$t('customer.relationAge')\">\r\n        <template slot-scope=\"{row}\">\r\n          <span>{{ row.relationAge }}</span>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column :label=\"$t('customer.relation')\">\r\n        <template slot-scope=\"{row}\">\r\n          <span>{{ row.relation}}</span>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column :label=\"$t('customer.relationContact')\">\r\n        <template slot-scope=\"{row}\">\r\n          <span>{{ row.relationContact }}</span>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column :label=\"$t('customer.unitName')\">\r\n        <template slot-scope=\"{row}\">\r\n          <span>{{ row.unitName }}</span>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column :label=\"$t('customer.unitNature')\">\r\n        <template slot-scope=\"{row}\">\r\n          <span>{{ row.unitNature }}</span>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column\r\n        :label=\"$t('customer.industry')\"\r\n        v-if=\"showUnitAddr\"\r\n      >\r\n        <template slot-scope=\"{row}\">\r\n          <span>{{ row.industry }}</span>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column\r\n        :label=\"$t('customer.educationLevel')\"\r\n        v-if=\"showDetailAddr\"\r\n      >\r\n        <template slot-scope=\"{row}\">\r\n          <span>{{ row.educationLevel }}</span>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column\r\n        :label=\"$t('customer.relationHobby')\"\r\n        v-if=\"showUnitContact\"\r\n      >\r\n        <template slot-scope=\"{row}\">\r\n          <span>{{ row.relationHobby }}</span>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column\r\n        :label=\"$t('customer.relationAddress')\"\r\n        v-if=\"showUnitPhone\"\r\n      >\r\n        <template slot-scope=\"{row}\">\r\n          <span>{{ row.educationLevel }}</span>\r\n        </template>\r\n      </el-table-column>\r\n      <el-table-column\r\n        :label=\"$t('common.operate')\"\r\n        min-width=\"100px\"\r\n        align=\"center\"\r\n        class-name=\"small-padding fixed-width\"\r\n      >\r\n        <template slot-scope=\"{row,$index}\">\r\n          <el-button\r\n            type=\"primary\"\r\n            size=\"mini\"\r\n            style=\"margin-right: 5px\"\r\n            @click=\"handleUpdate(row)\"\r\n          >\r\n            {{ $t('common.edit') }}\r\n          </el-button>\r\n          <el-popover\r\n            :ref=\"'popover-' + row.id\"\r\n            placement=\"top\"\r\n            width=\"160\"\r\n            :title=\"$t('common.confirm-delete')\"\r\n            trigger=\"click\"\r\n          >\r\n            <div style=\"text-align: right; margin: 0\">\r\n              <el-button\r\n                size=\"mini\"\r\n                @click=\"$refs[`popover-` + row.id].doClose()\"\r\n              >{{ $t('common.cancel') }}</el-button>\r\n              <el-button\r\n                type=\"primary\"\r\n                size=\"mini\"\r\n                @click=\"$refs[`popover-` + row.id].doClose();handleDelete(row,$index)\"\r\n              >{{ $t('common.confirm') }}</el-button>\r\n            </div>\r\n            <el-button\r\n              slot=\"reference\"\r\n              type=\"danger\"\r\n              size=\"mini\"\r\n            >\r\n              {{ $t('common.delete') }}\r\n            </el-button>\r\n          </el-popover>\r\n        </template>\r\n      </el-table-column>\r\n    </el-table>\r\n    <el-dialog\r\n      :title=\"textMap[dialogStatus]\"\r\n      :visible.sync=\"dialogFormVisible\"\r\n    >\r\n      <el-form\r\n        ref=\"dataForm\"\r\n        :rules=\"rules\"\r\n        label-position=\"right\"\r\n        label-width=\"100px\"\r\n        style=\"width: 100%\"\r\n      >\r\n        <el-form-item :label=\"$t('customer.relationName')\">\r\n          <el-input\r\n            v-model=\"temp.relationName\"\r\n            :placeholder=\"$t('common.enter')\"\r\n            minlength=\"3\"\r\n            maxlength=\"64\"\r\n            clearable\r\n          />\r\n        </el-form-item>\r\n        <el-form-item :label=\"$t('customer.relation')\">\r\n          <el-select\r\n            v-model=\"temp.relation\"\r\n            :placeholder=\"$t('common.choose')\"\r\n            clearable\r\n            class=\"filter-item\"\r\n            style=\"width: 100%\"\r\n          >\r\n            <el-option\r\n              v-for=\"item in NatureOptions\"\r\n              :key=\"item.value\"\r\n              :label=\"item.name\"\r\n              :value=\"item.value\"\r\n            />\r\n          </el-select>\r\n        </el-form-item>\r\n        <el-form-item :label=\"$t('customer.relationAge')\">\r\n          <el-input\r\n            v-model=\"temp.relationAge\"\r\n            :placeholder=\"$t('common.enter')\"\r\n            minlength=\"3\"\r\n            maxlength=\"64\"\r\n            clearable\r\n          />\r\n        </el-form-item>\r\n        <el-form-item :label=\"$t('customer.relationContact')\">\r\n          <el-input\r\n            v-model=\"temp.relationContact\"\r\n            :placeholder=\"$t('common.enter')\"\r\n            minlength=\"3\"\r\n            maxlength=\"64\"\r\n            clearable\r\n          />\r\n        </el-form-item>\r\n        <el-form-item :label=\"$t('customer.unitName')\">\r\n          <el-input\r\n            v-model=\"temp.unitName\"\r\n            :placeholder=\"$t('common.enter')\"\r\n            minlength=\"3\"\r\n            maxlength=\"64\"\r\n            clearable\r\n          />\r\n        </el-form-item>\r\n        <el-form-item :label=\"$t('customer.unitNature')\">\r\n          <el-input\r\n            v-model=\"temp.unitNature\"\r\n            :placeholder=\"$t('common.enter')\"\r\n            minlength=\"3\"\r\n            maxlength=\"64\"\r\n            clearable\r\n          />\r\n        </el-form-item>\r\n        <el-form-item :label=\"$t('customer.industry')\">\r\n          <el-input\r\n            v-model=\"temp.industry\"\r\n            :placeholder=\"$t('common.enter')\"\r\n            minlength=\"3\"\r\n            maxlength=\"64\"\r\n            clearable\r\n          />\r\n        </el-form-item>\r\n        <el-form-item\r\n          :label=\"$t('customer.educationLevel')\"\r\n          prop=\"unitPhone\"\r\n        >\r\n          <el-input\r\n            v-model=\"temp.educationLevel\"\r\n            :placeholder=\"$t('common.enter')\"\r\n            minlength=\"3\"\r\n            maxlength=\"64\"\r\n            clearable\r\n          />\r\n        </el-form-item>\r\n        <el-form-item :label=\"$t('customer.relationHobby')\">\r\n          <el-input\r\n            v-model=\"temp.relationHobby\"\r\n            :placeholder=\"$t('common.enter')\"\r\n            minlength=\"3\"\r\n            maxlength=\"64\"\r\n            clearable\r\n          />\r\n        </el-form-item>\r\n        <el-form-item :label=\"$t('customer.relationAddress')\">\r\n          <el-input\r\n            v-model=\"temp.relationAddress\"\r\n            :placeholder=\"$t('common.enter')\"\r\n            minlength=\"3\"\r\n            maxlength=\"64\"\r\n            clearable\r\n          />\r\n        </el-form-item>\r\n      </el-form>\r\n      <div\r\n        slot=\"footer\"\r\n        class=\"dialog-footer\"\r\n      >\r\n        <el-button @click=\"dialogFormVisible = false\">\r\n          {{ $t('common.cancel') }}\r\n        </el-button>\r\n        <el-button\r\n          type=\"primary\"\r\n          @click=\"dialogStatus==='create'?createData():updateData()\"\r\n        >\r\n          {{ $t('common.confirm') }}\r\n        </el-button>\r\n      </div>\r\n    </el-dialog>\r\n  </div>\r\n\r\n</template>\r\n\r\n<script>\r\nimport waves from '@/directive/waves' // waves directive\r\n// eslint-disable-next-line no-unused-vars\r\nimport { parseTime } from '@/utils'\r\n// eslint-disable-next-line no-unused-vars\r\nimport { getSociety, upSociety, delSociety, chaneSociety } from '@/api/getUser-info/society'\r\n\r\nexport default {\r\n\r\n  name: 'Work',\r\n  directives: { waves },\r\n  data () {\r\n    return {\r\n      list: [],\r\n      temp: {\r\n        id: undefined\r\n      },\r\n      dialogFormVisible: false,\r\n      dialogStatus: '',\r\n      textMap: {\r\n        update: this.$t('common.edit'),\r\n        create: this.$t('common.add')\r\n      },\r\n      rules: {\r\n\r\n      },\r\n      listLoading: false,\r\n      downloadLoading: false,\r\n      showUnitAddr: false,\r\n      showUnitContact: false,\r\n      showDetailAddr: false,\r\n      showUnitPhone: false\r\n    }\r\n  },\r\n\r\n  computed: {\r\n    NatureOptions () {\r\n      return [{\r\n        value: 'zhinan',\r\n        label: '指南'\r\n      }, {\r\n        value: 'jinan',\r\n        label: '济南'\r\n      }\r\n      ]\r\n    }\r\n  },\r\n  methods: {\r\n    resetTemp () {\r\n      this.temp = {\r\n        id: undefined\r\n      }\r\n    },\r\n    handleCreate () {\r\n      this.resetTemp()\r\n      this.dialogStatus = 'create'\r\n      this.dialogFormVisible = true\r\n      this.$nextTick(() => {\r\n        this.$refs['dataForm'].clearValidate()\r\n      })\r\n    },\r\n    // 新增工作经历\r\n    createData () {\r\n      // 转换为时间戳\r\n\r\n      if (this.temp.startTime !== undefined) {\r\n        this.temp['startTime'] = Date.parse(this.temp.startTime)\r\n      }\r\n      if (this.temp.endTime !== undefined) {\r\n        this.temp['endTime'] = Date.parse(this.temp.endTime)\r\n      }\r\n      this.temp['consumerId'] = '22332110'\r\n      upSociety(this.temp).then(data => {\r\n        this.getUserId()\r\n        this.dialogFormVisible = false\r\n      }).catch(err => {\r\n        console.log(err)\r\n      })\r\n    },\r\n    // 删除工作经历\r\n    handleDelete (row) {\r\n      delSociety({\r\n        relationId: row.id\r\n      }).then(data => {\r\n        console.log(data)\r\n        this.getUserId()\r\n      }).catch(err => {\r\n        console.log(err)\r\n      })\r\n    },\r\n    // 弹窗\r\n    handleUpdate (row) {\r\n      this.temp = Object.assign({}, row) // copy obj\r\n      this.dialogStatus = 'update'\r\n      this.dialogFormVisible = true\r\n      this.$nextTick(() => {\r\n        this.$refs['dataForm'].clearValidate()\r\n      })\r\n    },\r\n    // 修改工作经历\r\n    updateData () {\r\n      // 判断时间格式\r\n      console.log(this.temp)\r\n      delete this.temp['updateTime']\r\n      delete this.temp['isCurrent']\r\n      delete this.temp['createTime']\r\n\r\n      chaneSociety(\r\n        this.temp\r\n      ).then(data => {\r\n        console.log(data)\r\n        this.getUserId()\r\n        this.dialogFormVisible = false\r\n      }).catch(err => {\r\n        console.log(err)\r\n      })\r\n    },\r\n    // 获取工作经历信息\r\n    getUserId () {\r\n      getSociety({\r\n        consumerId: this.$route.params.id\r\n      }).then(data => {\r\n        console.log(data)\r\n        this.list = data.data\r\n        if (this.list[0]['endTime'] === null) {\r\n          this.list[0]['endTime'] = '在职'\r\n        }\r\n      }).catch(err => {\r\n        console.log(err)\r\n      })\r\n    },\r\n  },\r\n  mounted () {\r\n    this.getUserId()\r\n  }\r\n}\r\n\r\n</script>\r\n\r\n<style lang='less' scoped>\r\n</style>\r\n"]}]}